{
  "name": "chooslr-api",
  "version": "0.0.2",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "chooslr/api",
  "license": "MIT",
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "babel-register",
      "dotenv/config"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "test": "cross-env NODE_ENV=TEST nyc mocha -t 10000",
    "prebuild": "yarn test",
    "build": "rollup -c",
    "coverage": "nyc report --reporter=lcov && codecov",
    "nenv": "cross-env NODE_ENV=ENV node -r babel-register -r dotenv/config",
    "snap": "yarn nenv snap.js",
    "listen": "yarn nenv listen.js",
    "grant": "yarn nenv grant.js"
  },
  "dependencies": {
    "got": "^8.3.0",
    "grant-koa": "4.1.1",
    "jsonwebtoken": "^8.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-jwt": "^3.3.1",
    "koa-mount": "^3.0.0",
    "koa-proxies": "^0.7.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.1",
    "oauth-1.0a": "^2.2.4",
    "regenerator-runtime": "^0.11.1",
    "tiloop": "^1.0.1",
    "tumblrinbrowser": "^0.0.3",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-power-assert": "^2.0.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "cross-fetch": "^2.2.0",
    "dotenv": "^5.0.1",
    "fs-extra": "^5.0.0",
    "grant-just": "https://github.com/kthjm/grant-just",
    "jsdom": "^11.6.2",
    "koa-logger": "^3.2.0",
    "mocha": "^5.0.2",
    "npm-run-all": "^4.1.3",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rewire": "^3.0.2",
    "rollup": "^0.56.4",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-uglify": "^3.0.0",
    "supertest": "^3.0.0",
    "uglify-es": "^3.3.9"
  }
